<h1>Shopping Cart</h1>

<% if (data.length > 0) { %>
  <ul>
    <% data.forEach(cartItem => { %>
      <li>
        <h3><%= cartItem.carted.name %></h3>
        <p>Price: $<%= cartItem.carted.price %></p>
        <p>Quantity: <%= cartItem.quantity %></p>
        <p>Subtotal: $<%= cartItem.quantity * cartItem.carted.price %></p>
      </li>
    <% }); %>
  </ul>
<% } else { %>
  <p>Cart is empty.</p>
<% } %>